bs_package_flags = -bs-package-name client -bs-package-map Client
src_root_dir = /home/tom/haskell-poker/client
bsc = /home/tom/haskell-poker/client/node_modules/bs-platform/lib/bsc.exe
bsdep = /home/tom/haskell-poker/client/node_modules/bs-platform/lib/bsb_helper.exe
warnings = -w -30-40+6+7+27+32..39+44+45+101
bsc_flags = -bs-suffix -nostdlib -I '/home/tom/haskell-poker/client/node_modules/bs-platform/lib/ocaml' -color always
ppx_flags = 
bs_package_includes = -I /home/tom/haskell-poker/client/node_modules/reason-react/lib/ocaml -I /home/tom/haskell-poker/client/node_modules/bs-fetch/lib/ocaml -I /home/tom/haskell-poker/client/node_modules/@glennsl/bs-json/lib/ocaml -I /home/tom/haskell-poker/client/node_modules/relude/lib/ocaml -I /home/tom/haskell-poker/client/node_modules/bs-abstract/lib/ocaml
bs_package_dev_includes = 
namespace = -ns Client
bsb_dir_group = 0
refmt = /home/tom/haskell-poker/client/node_modules/bs-platform/lib/refmt.exe
reason_react_jsx = -bs-jsx 2
refmt_flags = --print binary
bsc_lib_includes = -I . -I app/components -I app
rule build_ast_and_module_sets_from_re
  command = ${bsc} -pp "${refmt} ${refmt_flags}" ${reason_react_jsx}  ${ppx_flags} ${warnings} ${bsc_flags} -c -o ${out} -bs-syntax-only -bs-binary-ast -impl ${in}
  description = [34mBuilding[39m [2m${out}[22m
build app/Api.mlast : build_ast_and_module_sets_from_re $src_root_dir/app/Api.re
rule build_deps
  command = ${bsdep} ${namespace} -g ${bsb_dir_group} -MD ${in}
  restat = 1
  description = [34mBuilding[39m [2m${out}[22m
build app/Api.mlast.d : build_deps app/Api.mlast
rule build_cmj_cmi
  command = ${bsc} ${bs_package_flags} -bs-assume-no-mli -bs-no-builtin-ppx-ml -bs-no-implicit-include ${bs_package_includes} ${bsc_lib_includes} ${bsc_extra_includes} ${warnings} ${bsc_flags} ${gentypeconfig} -o ${out} -c  ${in} $postbuild
  depfile = ${in}.d
  restat = 1
  description = [34mBuilding[39m [2m${out}[22m
build app/Api-Client.cmj |  $src_root_dir/app/Api.bs.js app/Api-Client.cmi : build_cmj_cmi app/Api.mlast
  bsc_flags = $bsc_flags -bs-re-out -bs-super-errors
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:app
build app/types.mlast : build_ast_and_module_sets_from_re $src_root_dir/app/types.re
build app/types.mlast.d : build_deps app/types.mlast
build app/types-Client.cmj |  $src_root_dir/app/types.bs.js app/types-Client.cmi : build_cmj_cmi app/types.mlast
  bsc_flags = $bsc_flags -bs-re-out -bs-super-errors
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:app
build app/components/Counter.mlast : build_ast_and_module_sets_from_re $src_root_dir/app/components/Counter.re
build app/components/Counter.mlast.d : build_deps app/components/Counter.mlast
build app/components/Counter-Client.cmj |  $src_root_dir/app/components/Counter.bs.js app/components/Counter-Client.cmi : build_cmj_cmi app/components/Counter.mlast
  bsc_flags = $bsc_flags -bs-re-out -bs-super-errors
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:app/components
rule build_package
  command = ${bsc} -w -49 -no-alias-deps -bs-cmi-only -c ${in}
  restat = 1
  description = [34mBuilding[39m [2m${out}[22m
build Client.cmi : build_package Client.mlmap
build build.ninja : phony  ||  Client.cmi app/components/Counter.mlast.d app/types.mlast.d app/Api.mlast.d
